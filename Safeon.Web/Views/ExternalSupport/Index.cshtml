<!-- Page-Title -->
<div class="row">
    <div class="col-sm-12">
        <h3 class="page-header float-left">Agentes Externos</h3>
        <div class="text-lg-right" style="padding-top:8px;">
            <a class="btn btn-primary btn-rounded w-md waves-effect waves-light" href="/ExternalSupport/Create">
                <i class="ace-icon fa fa-plus-circle"></i>
                Novo Agente Externo
            </a>
        </div>
        <hr />
        <div class="row">
            <div class="col-sm-12">
                <div class="card">
                    <div class="card-body table-responsive">
                        <table id="table-external-support" class="table table-striped table-bordered dt-responsive nowrap" 
                               style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                            <thead>
                                <tr>
                                    <th>Nome</th>
                                    <th>Cidade</th>
                                    <th>Latitude</th>
                                    <th>Longitude</th>
                                    <th>Ações</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
